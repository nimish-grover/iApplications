   {%macro steps(progress=None)%}
   <div class="main-container">
     <div class="steps-container">
         {%for label,data in progress.items() %}
         <div class="step {%if data.isApproved %} completed {%endif%}{%if data.current%} in-progress{%endif%}">
             {%if data.isApproved %}
             <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                 <path d="M20.285 2l-11.285 11.567-5.286-5.011-3.714 3.716 9 8.728 15-15.285z" />
             </svg>
             {%endif%}
             {%if data.current%}
             <div class="preloader"></div>
             {%endif%}
             <div class="label {%if data.isApproved %} completed {%endif%}{%if data.current%} loading {%endif%}">
                 {{label|title()}}
             </div>
             <div class="icon {%if data.isApproved %} completed {%endif%}{%if data.current%} in-progress {%endif%}">
                 <i class="{{data.icon}}"></i>
             </div>
         </div>
         {%if not loop.last%}
         <div class="line"></div>
         {%endif%}
         {%endfor%}
     </div>
 </div>
 {%endmacro%}